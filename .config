---
antden:
  scheduler:
    db: $ROOT/scheduler/conf/scheduler.db
    code: $ROOT/scheduler/code
    conf: $ROOT/scheduler/conf
  controller:
    db: $ROOT/scheduler/conf/scheduler.db
    code: $ROOT/controller/code
    conf: $ROOT/scheduler/conf
  slave:
    db: $ROOT/slave/conf/slave.db
    task: $ROOT/slave/conf/task
    code: $ROOT/slave/code

agent:
  path: $MYDanPATH/dan/agent/path
  argv: $MYDanPATH/dan/agent/argv
  auth: $MYDanPATH/etc/agent/auth
  proxy: $MYDanPATH/etc/agent/proxy
  conf: $MYDanPATH/dan/agent/conf
  lib:  $MYDanPATH/dan/agent/lib
  port: 65111
  role: agent #myrole


